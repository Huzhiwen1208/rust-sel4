[unstable]
unstable-options = true
build-std = ["core", "alloc", "compiler_builtins"]
build-std-features = ["compiler-builtins-mem"]

[target.x86_64-unknown-none]
rustflags = [
    # NOTE
    # An alternative would be creating a new target spec based on x86_64-unknown-none with
    # position_independent_executables and/or static_position_independent_executables set to false
    "-Crelocation-model=static",
]
